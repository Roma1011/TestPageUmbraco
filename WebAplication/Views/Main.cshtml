@inherits Umbraco.Web.Mvc.UmbracoViewPage
@using ClientDependency.Core.Mvc
@{
	Layout = null;
	var homePage=Model.AncestorOrSelf<Home>();//Umbraco Web PublishedModels Home
}
<html>
<!--- scripts,css etc---->
<head>
	@{
		<!--CSS-->
		Html.RequiresCss("https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css", new
		{
		integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm",
		crossorigin="anonymous"
		});

		Html.RequiresCss("https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css");
		Html.RequiresCss("/css/CookieConsent.css");
		<!--JQ-->
		Html.RequiresJs("https://code.jquery.com/jquery-3.2.1.slim.min.js",new
		{
		integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN",
		crossorigin="anonymous"
		});
		<!--JS-->
		Html.RequiresJs("https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js",new
		{
		integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q",
		crossorigin="anonymous"
		});
		<!--Bootstrap-->
		Html.RequiresJs("https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js",new
		{
		integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl",
		crossorigin="anonymous"
		});
		<!--CDNFONTAWESOME-->
		Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/js/all.min.js");
		Html.RequiresJs("/scripts/CookieConsent.js");
	}
	@Html.RenderCssHere()
</head>
<body>
	<!--NavBar-->
	@Html.Partial("PageSections/NavBar")
	@RenderBody()
	<!--Footer-->
	@Html.Partial("PageSections/Footer")
	<div class="alert alert-dismissible text-center cookiealert" role="alert">
		<div class="cookiealert-container">
			<b>Do you like cookies?</b> &#x1F36A; We use cookies to ensure you get the best experience on our website. <a href="http://cookiesandyou.com/" target="_blank">Learn more</a>

			<button type="button" class="btn btn-primary btn-sm acceptcookies" aria-label="Close">
				I agree
			</button>
		</div>
	</div>
	@Html.RenderJsHere()
</body>
</html>